        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Risol"
        android:textColor="@color/text"
        android:textSize="20dp"
        app:layout_constraintTop_toBottomOf="@id/img_kue1"
        app:layout_constraintRight_toRightOf="@id/img_kue1"
        app:layout_constraintLeft_toLeftOf="@id/img_kue1"/>
    <TextView
        android:id="@+id/tv_kuehar1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rp.2.000/Pcs"
        android:textColor="@color/text"
        android:textSize="15dp"
        app:layout_constraintTop_toBottomOf="@id/tv_kue1"
        app:layout_constraintRight_toRightOf="@id/tv_kue1"
        app:layout_constraintLeft_toLeftOf="@id/tv_kue1"/>

    <TextView
        android:id="@+id/tv_kue2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Putri Ayu"
        android:textColor="@color/text"
        android:textSize="20dp"
        app:layout_constraintTop_toBottomOf="@id/img_kue2"
        app:layout_constraintRight_toRightOf="@id/img_kue2"
        app:layout_constraintLeft_toLeftOf="@id/img_kue2"/>
    <TextView
