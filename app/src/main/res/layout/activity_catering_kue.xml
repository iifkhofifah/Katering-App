        android:textSize="15dp"
        app:layout_constraintTop_toBottomOf="@id/tv_kue4"
        app:layout_constraintRight_toRightOf="@id/tv_kue4"
        app:layout_constraintLeft_toLeftOf="@id/tv_kue4"/>
